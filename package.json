{
  "name": "create-went-app",
  "version": "0.0.2",
  "description": "A CLI tool for generating Next.js projects with authentication, database setup, and modern tooling",
  "main": "dist/index.js",
  "bin": {
    "went": "./dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "nextjs",
    "react",
    "typescript",
    "authentication",
    "prisma",
    "tailwind",
    "template",
    "generator",
    "boilerplate"
  ],
  "author": "Felix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixhard/went"
  },
  "homepage": "https://github.com/felixhard/went#readme",
  "bugs": {
    "url": "https://github.com/felixhard/went/issues"
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.29",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.2",
    "inquirer": "^12.6.3",
    "ora": "^8.2.0",
    "ts-node": "^10.9.2"
  }
}
